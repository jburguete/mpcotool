MOES = $(LOCALE)es/LC_MESSAGES/gtk30.mo
MOFR = $(LOCALE)fr/LC_MESSAGES/gtk30.mo
MOGB = $(LOCALE)en_GB/LC_MESSAGES/gtk30.mo
