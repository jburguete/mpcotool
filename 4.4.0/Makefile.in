	$(BIN)libbz2-1.dll\
	$(BIN)libcairo-2.dll\
	$(BIN)libcairo-gobject-2.dll\
	$(BIN)libcroco-0.6-3.dll\
	$(BIN)libepoxy-0.dll\
	$(BIN)libexpat-1.dll\
	$(BIN)libffi-6.dll\
	$(BIN)libfontconfig-1.dll\
	$(BIN)libfreetype-6.dll\
	$(BIN)libgcc_s_*.dll\
	$(BIN)libgdk_pixbuf-2.0-0.dll\
	$(BIN)libgdk-3-0.dll\
